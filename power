// pow of numbers

#include<iostream>
using namespace std;

int pow(int m,int n)
{

   if(n>0)
   {
  
     return m*pow(m,n-1);

   }

   else if(n==0)

   {

     return 1;         

   }

   else
   {

     exit(-1);

   }

}

int main()

{

 int n;
 int m;
 cin>>m>>n;
 cout<<"the pow of numbers"<<m<<" ^ "<<n<<"="<<endl<<pow(m,n)<<endl;

return 0;

}
