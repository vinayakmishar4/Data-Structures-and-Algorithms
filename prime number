#include <iostream>
using namespace std;

int main()
{
  
   int n;
   int i;
   int m=0;
   int flag=0;

   cout<<"enter the number"<<endl;
   cin>>n;
   m=n/2;

   for(int i=2;i<=m;i++)
   {
     if(n%i==0)
     {

       cout<<"number is not prime number"<<endl;
       flag=1;
       break;
     }
     if(flag==0)
     {

       
       cout<<"number is  prime number"<<endl;
       return 0;
       break;

     }
    


     
   



   } 







}
